{
  "expo": {
    "name": "JoudExperiment",
    "slug": "Joud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/i.png",
    "splash": {
      "image": "./assets/Splash_Background.png",
      "resizeMode": "cover",
      "backgroundColor": "#D9E8F1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shahad23.s"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
